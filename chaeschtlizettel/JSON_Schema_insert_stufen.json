{
   "definitions": {},
   "$schema": "",
   "$id": "https://github.com/Cevi-Uster/infobox/tree/master/chaeschtlizettel/JSON_Schema_insert_stufen.json",
   "type": "object",
   "title": "Update Stufe Schema",
   "default": null,
   "required": [
     "name"
   ],
   "properties": {
     "name": {
       "$id": "#/properties/name",
       "type": "string",
       "title": "Stufenname",
       "default": "",
       "examples": [
         "Jabalaya"
       ],
       "pattern": "^(.*)$"
     }
   },
   "abteilung": {
       "$id": "#/properties/abteilung",
       "type": "string",
       "title": "Abteilung",
       "default": "",
       "examples": [
         "m", "f"
       ],
       "pattern": "^(.*)$"
    },
    "jahrgang": {
       "$id": "#/properties/jahrgang",
       "type": "integer",
       "title": "Jahrgang",
       "default": "",
       "examples": [
         "2011"
       ]
    },
    "email": {
       "$id": "#/properties/email",
       "type": "string",
       "title": "E-Mail",
       "default": "",
       "examples": [
         "test@example.com"
       ],
       "format": "email"
    }
}
